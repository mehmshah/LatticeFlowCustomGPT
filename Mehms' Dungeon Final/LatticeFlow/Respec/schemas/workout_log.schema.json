{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Workout Log",
  "type": "object",
  "required": [
    "date",
    "day",
    "title",
    "rpe_summary"
  ],
  "properties": {
    "date": {
      "type": "string",
      "pattern": "^\\\\d{4}-\\\\d{2}-\\\\d{2}$"
    },
    "day": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "mood": {
      "type": "object",
      "properties": {
        "pre": {
          "type": "integer"
        },
        "post": {
          "type": "integer"
        }
      }
    },
    "energy": {
      "type": "object",
      "properties": {
        "pre": {
          "type": "integer"
        },
        "post": {
          "type": "integer"
        }
      }
    },
    "rest": {
      "type": "string"
    },
    "rpe_summary": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    },
    "weights": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    },
    "notes": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}